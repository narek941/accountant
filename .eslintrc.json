{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "ignorePatterns": [
    "dist/"
  ],
  "rules": {
    "no-func-assign": "off",
    "no-unused-vars": ["error", { "args": "none" }],
    "react/no-unused-vars": "off",
    "react/no-unknown-property": "off"
  },
  "globals": {
    "process": true
  }
}
